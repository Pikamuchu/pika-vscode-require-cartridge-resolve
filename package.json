{
  "name": "require-cartridge-resolve",
  "displayName": "Require cartridge resolve",
  "description": "Provides definitions for SFRA cartridge requires in your code for quick navigation.",
  "version": "1.0.0",
  "publisher": "pikamachu",
  "repository": "https://github.com/pikamachu/pika-vscode-ext-require-cartidge-resolve",
  "engines": {
    "vscode": "^1.25.0"
  },
  "icon": "images/icon.png",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "go to definition",
    "require",
    "cartridge",
    "SFRA"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:isml"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test",
    "prettier": "prettier --parser typescript --print-width 120 --write \"./**/*.ts\""
  },
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Require cartridge resolve",
      "properties": {
        "require.cartridge.resolve.filetypes": {
          "type": "string",
          "default": "js,isml",
          "description": "File types where the extension should be enabled."
        }
      }
    }
  },
  "license": "MIT",
  "homepage": "https://github.com/pikamachu/pika-vscode-ext-require-cartidge-resolve",
  "devDependencies": {
    "@types/node": "8.10.25",
    "@types/mocha": "5.0.0",
    "mocha": "5.0.1",
    "prettier": "1.15.3",
    "tslint": "5.11.0",
    "typescript": "3.0.3",
    "vscode": "1.1.22"
  }
}
